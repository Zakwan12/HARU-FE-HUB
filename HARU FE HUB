local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "H.R.U FE SCRIPT", HidePremium = false, IntroText = "H.R.U Hub",SaveConfig = true, ConfigFolder = "TutorialConfig"})

OrionLib:MakeNotification({
	Name = "WELCOME",
	Content = "WELCOME TO HRU FE HUB HEHE :3",
	Image = "rbxassetid://10100630346",
	Time = 10
})

--values

     
--tabs
